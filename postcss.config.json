{
	"plugins": [
		"postcss-flexbugs-fixes",
		"postcss-normalize",
		[
			"postcss-preset-env",
			{
				"autoprefixer": {
					"flexbox": "no-2009"
				},
				"stage": 3,
				"features": {
					"custom-properties": false
				}
			}
		],
		[
			"postcss-fontpath",
			{
				"formats": [
					{ "type": "woff2", "ext": "woff2" },
					{ "type": "truetype", "ext": "ttf" },
					{ "type": "opentype", "ext": "otf" }
				]
			}
		]
	]
}
